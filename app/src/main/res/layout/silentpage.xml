<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnsalahtiprayer"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecyan_100">

            <ScrollView
                android:id="@+id/scrollViewScrollview"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:clipToPadding="false"
                android:fillViewport="true"
                android:gravity="start">

                <LinearLayout
                    android:id="@+id/linearSplash"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:orientation="vertical">






                    <ImageView
                        android:id="@+id/imageRectangleTwentyFour"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_rectangle24"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_100pxv"
                        android:layout_gravity="bottom|start"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle24" />


                    <FrameLayout
                        android:id="@+id/frameStackvector"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_471pxv"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:gravity="start">

                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/StarttimeB"
                            style="@style/btnSolidRounded_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_40pxh"
                            android:layout_marginTop="@dimen/_280pxh"
                            android:layout_marginBottom="@dimen/_100pxv"
                            android:text="Start time"
                            />

                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/EndtimeB"
                            style="@style/btnSolidRounded_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_250pxh"
                            android:layout_marginTop="@dimen/_280pxh"
                            android:layout_marginBottom="@dimen/_100pxv"
                            android:text=" End time "
                            />

                        <TextView
                            android:id="@+id/StarttextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="TextView"
                            android:layout_marginLeft="@dimen/_52pxh"
                            android:layout_marginTop="@dimen/_400pxv" />

                        <TextView
                            android:id="@+id/EndtextView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="TextView"
                            android:layout_marginLeft="@dimen/_267pxh"
                            android:layout_marginTop="@dimen/_400pxv" />/>






                    </FrameLayout>




                </LinearLayout>
            </ScrollView>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>









